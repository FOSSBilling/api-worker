{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:best-practices",
    "schedule:weekly",
    "security:minimumReleaseAgeNpm",
    ":automergeBranch",
    ":automergeDigest",
    ":automergeLinters",
    ":automergePatch",
    ":automergeRequireAllStatusChecks",
    ":automergeTesters",
    ":automergeTypes",
    ":maintainLockFilesWeekly",
    ":npm",
    ":rebaseStalePrs"
  ],
  "automergeSchedule": [
    "at any time"
  ],
  "automergeStrategy": "squash",
  "postUpdateOptions": [
    "npmDedupe"
  ],
  "skipInstalls": false
}